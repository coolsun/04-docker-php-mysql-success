.jobnote .content ol, .jobnote .content ul { padding-left: 40px; }

.view .jobnote { border-bottom: none; }
.view .jobnote .meta .date { display: block; float: none; margin-top: 12px; color: #a3a3a3; }

.edit-area { width: 700px; padding: 0px 5px; margin: 40px auto 0px auto; }
.edit-area input { border: 1px solid #c3c3c3; padding: 4.5px 0px; padding-left: 5px; width: 100%; }
.edit-area .note-content { display: none; }
.edit-area .jqte_editor { height: 470px; resize: none; }
.edit-area a.submit { display: inline-block; padding: 5px 12px; }