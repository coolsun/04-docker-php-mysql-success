/*
 Map Table
*/
.map_table { border-left: 1px solid #c3c3c3; border-right: 1px solid #c3c3c3; table-layout: fixed;  border-collapse: separate; border-spacing: 0; }
.map_table td.column  { border: 1px solid #c3c3c3; border-left: none; border-top: none;  padding: 5px 0; box-shadow: -1px 1px 0 rgba(255,255,255,0.6) inset; text-align: center; }
.map_table thead { text-align:left; width: 100%; }
.map_table thead tr {  width: 100%; }
.map_table thead tr.days td:first-child,
.map_table thead tr.dates td:first-child,
.map_table thead tr.days td:last-child,
.map_table thead tr.dates td:last-child,
.map_table tbody tr,
.map_table tbody tr td {  max-height: 24px; height: 24px; }
.map_table tbody tr td { border-right: 1px #c3c3c3 solid;  border-bottom: 1px dashed #c3c3c3; line-height: 23px; }
.map_table tbody tr td:last-child { border-right: none; }
.map_table tbody tr:last-child td { border-bottom: none; }
.map_table tbody .job,
.map_table tbody .subjob { max-height: 22px; height: 22px; }
.map_table tbody .subjob { padding-left: 14px; }
.map_table tbody .addSession { padding-left: 5px; color: #838383; cursor: pointer; }
.map_table tbody .session { padding-left: 5px; border-bottom: 1px dashed #c3c3c3; }
.map_table tbody .session.hasSession,
.map_table tbody .session.newSession { height: 48px; position: relative; }
.map_table tbody td .session:last-child { border-bottom: none; }
.map_table tbody td .session.newSession .session_input { padding-left: 5px; width: 113px; border: 1px solid #c3c3c3;  }
.map_table tbody td .session.newSession .cancelNewSession,
.map_table tbody td .session.hasSession .deleteSession { display: none; }
.map_table tbody td .session.newSession:hover .cancelNewSession,
.map_table tbody td .session.hasSession:hover .deleteSession { display: inline-block; position: absolute; top:0; right: 0; background-color: #c3c3c3; height: 12px; width: 10px; line-height: 10px; font-size: 10px; text-align: center; cursor: pointer; }
.map_table tbody td .session.newSession:hover .cancelNewSession:active,
.map_table tbody td .session.hasSession:hover .deleteSession:active { background: red; }
.map_table tbody td .session.hasSession .session_input { padding-left: 5px; width: 113px; border: 1px solid transparent; color: #555; }
.map_table tbody td .session.hasSession .session_input:focus { border-color: #c3c3c3; background-color: #e3e3e3;   }
.map_table tfoot tr td:first-child { border-left: none; }
.map_table tfoot tr td:last-child { border-right: none; }
.map_table tfoot tr { background-color: #E5E5E5; }

.map_table_jobs_scrollalbe { height: 456px; overflow-x: hidden; overflow-y: auto; }
.map_table_session_scrollalbe { height: 143px; overflow-x: hidden; overflow-y: auto;}

