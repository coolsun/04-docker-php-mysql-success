/*
 Common class
*/
.error_msg { text-align: center; color: red; }

.center { margin: 0 auto; }

.inlinebox { display: inline-block; vertical-align: middle; padding: 0; }

.text-hidden { white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }

/*
 Placeholder text
*/
input::-webkit-input-placeholder { /* WebKit browsers */
    color:    #777;
}
input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color:    #777;
}
input::-moz-placeholder { /* Mozilla Firefox 19+ */
    color:    #777;
}
input:-ms-input-placeholder { /* Internet Explorer 10+ */
    color:    #777;
}
.placeholdersjs { /* For IE */
	color: #AAA; 
}
input:focus::-webkit-input-placeholder { color:transparent; }
input:focus::-moz-placeholder { color:transparent; }


#main-content * {
	font-size: 14px;
	font-family: Arial;
	-moz-box-sizing: border-box; -webkit-box-sizing: border-box; -ms-box-sizing: border-box; box-sizing: border-box;
}

/*
 Button
*/
a.button, a.picbutton { display: inline-block; text-align: center; }
a.button { padding: 2px 12px; border: 1px solid #939393; color: #3c3c3c; background-color: #E5E5E5; box-shadow: -1px 1px 0 rgba(255,255,255,0.5) inset; /* For IE8 */ behavior: url(ie-css3.htc); }
a.button:hover { background-color: #f0f0f0; }
a.button:active { background-color: #d5d5d5; }
a.picbutton { height: 25px; line-height: 25px; width: 70px; color: #fff; background: url("../img/btn_bg.gif") no-repeat top; filter:alpha(opacity=90); -moz-opacity:0.9; -khtml-opacity: 0.9; opacity: 0.9; }
@media \0screen {
	a.picbutton {line-height:25px;} /* IE8- */
} 
a.picbutton:hover { filter:alpha(opacity=80); -moz-opacity: 0.8; -khtml-opacity: 0.8; opacity: 0.8;  }
a.picbutton:active { filter:alpha(opacity=100); -moz-opacity: 1; -khtml-opacity: 1; opacity: 1; box-shadow: 1px 1px 1px rgba(255,255,255,0.3) inset, -1px -1px 1px rgba(255,255,255,0.3) inset; }

/*
 Date Picker
*/
.during-view > span { display: inline-block; vertical-align: middle; height: 25px; line-height: 25px; }
.pick-during { display: none; border: 1px solid #c3c3c3; width: 180px; background-color: #DEDEDE; position: absolute; top: 0; left: 584px; z-index: 999; padding: 3px; box-shadow: 1px 1px 1px rgba(255,255,255,0.5) inset; line-height: normal; }
a.closex { background: url('../img/closex.png') no-repeat; }
a.closex:active { background: url('../img/closex_active.png'); }
.pick-during a.closex { display: inline-block; height: 13px; width: 16px; }
.pick-during > div.during-box { background-color: white; width: auto; margin-top: 5px; padding: 5px; border: 1px solid #c3c3c3; border-radius: 1px; }
.pick-during > div.during-box > div { margin-top: 5px; width: 100%; }
.pick-during input[readonly].date-pick { width: 80px; font-family: Arial; font-size: 13px; }
.during-submit { width: 100%; text-align: right; padding: 2px 0px; padding-right: 8px; }
.during-submit > a { display: inline-block; padding: 3px 8px; border: 1px solid #c3c3c3; background: #DEDEDE; box-shadow: -1px 1px 1px rgba(255,255,255,0.5) inset; }

input[readonly] { background-color: white; cursor: pointer; }

/*
 Table
*/
.data_table { border: 1px solid #c3c3c3; border-bottom: none;  border-spacing: 0; border-collapse:separate; border-radius: 3px 3px 0 0; }
.data_table thead tr { background-color: #E5E5E5; }
.data_table thead select { border: 1px solid #b0b0b0; }
.data_table thead tr:first-child td { padding: 3px; box-shadow: 0 1px 0 rgba(255,255,255,0.5) inset; border-bottom: 1px solid #c3c3c3; }
.data_table thead tr:first-child td:last-child { box-shadow: -1px 1px 0 rgba(255,255,255,0.5) inset; text-align: right; }
.data_table thead a.button {  line-height: 20px; border: 1px solid #a3a3a3; color: #606060; background-color: #E5E5E5; box-shadow: -1px 1px 0 rgba(255,255,255,0.5) inset; }
.data_table thead a.button:hover { background-color: #f3f3f3; color: #3c3c3c; box-shadow: none; }
.data_table thead a.button:active { background-color: #939393; color: #f9f9f9; box-shadow: none; border: 1px solid #737373; }
.data_table thead a.on { background-color: #979797; color: #f3f3f3; border: 1px solid #838383; box-shadow: none; box-shadow: -1px 1px 0 rgba(179,179,179,1) inset; }
.data_table thead td.thead { padding: 5px 0px; border-bottom: 1px solid #c3c3c3; border-right: 1px solid #c3c3c3; box-shadow: -1px 1px 0 rgba(255,255,255,0.5) inset; text-align: center; }
.data_table thead tr td.thead:last-child { box-shadow: -1px 1px 0 rgba(255,255,255,0.5) inset; }
.data_table tbody.twoColorRow tr.grayLine { background-color: #f0f0f0; }
/*.data_table tbody.twoColorRow tr td { border-right: 1px solid #c3c3c3; }*/
/*.data_table tbody.twoColorRow tr td > * { vertical-align: middle; }*/
.data_table_tail { height: 5px; background-color: #e5e5e5; border: 1px solid #c3c3c3; border-top: none; border-radius: 0 0 3px 3px; /* For IE8 */ behavior: url(PIE.htc); }

/*
 Select element
*/
.noarrow_select { -webkit-appearance: none; -moz-appearance: none; appearance: none; border: none; background-color: transparent; }

/*
 Select List
*/
.selectList { width: 90%; position: relative; }
.selectList ul.list { display: none; position: absolute; width: 100%; background-color: #fff; z-index: 2; }
.selectList:active ul.list { display: inherit; }
.selectList li { list-style: none; text-align: center; background-color: #fff; }


